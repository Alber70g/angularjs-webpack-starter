        "ngtemplate-loader": "^1.3.1",
        "karma-firefox-launcher": "0.1.x",
        "karma-junit-reporter": "0.3.x",
        "karma-spec-reporter": "0.0.x",
        "karma-mocha-reporter": "^2.0.0",
        "karma-ie-launcher": "0.2.x",
